(define (add [x i64] [y i64]) i64
  (block (start)
    (set r (add i64 x y))
    (ret r)
  )
)

(define (entry) void
  (block (start)
    (set r (call i64 (add (const i64 1) (const i64 2))))
    (call void (assert_eq_u64 r (const i64 3)))
    (set r (call i64 (add (const i64 5) (const i64 2))))
    (call void (assert_eq_u64 r (const i64 7)))
    (ret)
  )
)
